<p id="notice"><%= notice %></p>

<h1>Test Configs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Short name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @test_configs.each do |test_config| %>
      <tr>
        <td><%= test_config.name %></td>
        <td><%= test_config.short_name %></td>
        <td><%= link_to 'Show', test_config %></td>
        <td><%= link_to 'Edit', edit_test_config_path(test_config) %></td>
        <td><%= link_to 'Destroy', test_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Test Config', new_test_config_path %>
