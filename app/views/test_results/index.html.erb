<p id="notice"><%= notice %></p>

<h1>Test Results</h1>

<table>
  <thead>
    <tr>
      <th>Ext</th>
      <th>Tests count</th>
      <th>Tests passed</th>
      <th>Tests failed</th>
      <th>Tests skipped</th>
      <th>Release</th>
      <th>Split</th>
      <th>Config</th>
      <th>Asssignee</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @test_results.each do |test_result| %>
      <tr>
        <td><%= test_result.ext_id %></td>
        <td><%= test_result.tests_count %></td>
        <td><%= test_result.tests_passed %></td>
        <td><%= test_result.tests_failed %></td>
        <td><%= test_result.tests_skipped %></td>
        <td><%= test_result.release_id %></td>
        <td><%= test_result.split_id %></td>
        <td><%= test_result.config_id %></td>
        <td><%= test_result.asssignee_id %></td>
        <td><%= link_to 'Show', test_result %></td>
        <td><%= link_to 'Edit', edit_test_result_path(test_result) %></td>
        <td><%= link_to 'Destroy', test_result, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Test Result', new_test_result_path %>
